*
  box-shadow 0 0 30px #000
  color #f5f5f5
  font-size 25px
  padding 3px
  transition all .3s ease

:not(input):not(h1):not(h4):not(div):not(span):not(p)
  background-image linear-gradient(#008000 10%,#fff,#f5f5f5,#808080,#000,#228b22)

input,h4
  background-color inherit
  box-shadow 0 0 30px #000
  padding 3px
  text-align center
  border none

input[type=button]
  background-image radial-gradient(#228b22 7%,#006400,#000)
  color #f5f5f5
  cursor pointer
  width 1.9%

#bracken input,#backAnime
  height 29px
  width 22.8%
  outline-color #f00

#backAnime
  background transparent
  border outset 2px #9acd05
  display none
  opacity .6
  position absolute
  height 35px
  transition position 0 ease

img
  background-image radial-gradient(circle,#a9a9a9,#808080,#f5f5f5)
  box-shadow 0 0 30px #ff0
  border 20px groove #9400d3
  height 200px
  width 180px
  position relative
  left 5%
  transition all 1s ease-in-out
  -webkit-transform translateX(-30%)
  -moz-transform translateX(-30%)

img:hover
  border-radius 50px
  left -.66%
  height 410px
  width 50%
  -webkit-transform translateX(0%)
  -moz-transform translateX(0%)

#luis,#fern,#fern input:not(#eks):not(#OSTroot)
  box-shadow 0 0 0 transparent

#fern input
  border 34px
  border-radius 30%
  outline-color #f00
  width 30px

#fern input[type=button]
  outline none
  width 30px

#fern inputfocus,input[type=text]:hover
  background-image radial-gradient(#000,#a9a9a9,#808080,#a9a9a9,#000)
  box-shadow 0 0 30px #000

#fern input[type=text]
  cursor text

#luis
  background transparent
  position absolute

#eks
  position absolute
  left 20%
  top 80.4%

#OSTroot
  outline none

#OSTrootfocus
  box-shadow 0 0 30px #f5f5f5

#Afocus,#dAfocus
  outline-color #0ff

#Fifthfocus,#dFifthfocus
  outline-color #0c9

#Bfocus,#dBfocus
  outline-color #00f

#Fourthfocus,#dFourthfocus
  outline-color #06f

#Cfocus,#dCfocus
  outline-color #f00

#Cubedfocus,#dCubedfocus
  outline-color #903

#Dfocus,#dDfocus
  outline-color #adff2f

#Squaredfocus,#dSquaredfocus
  outline-color #390

#Efocus,#dEfocus
  outline-color #daa520

#Constantfocus
  outline-color #600

sub
  background transparent
  box-shadow 0 0 0 #f0e68c

undefined